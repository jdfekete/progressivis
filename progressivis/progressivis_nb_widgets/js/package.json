{
  "name": "progressivis-nb-widgets",
  "version": "0.1.0",
  "description": "A Custom Jupyter Widget Library for Progressivis",
  "author": "Jean-Daniel Fekete",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdfekete/progressivis/progressivis_nb_widgets"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "css/**/*.css",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "prepublish": "npm run build:babel",
    "build:babel": "rm -rf lib/*.js && babel src/*.js --out-dir lib",
    "build": "npm run build:babel && webpack",  
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "rimraf": "^2.6.1",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2",
    "chroma-js": "",
    "core-js": "^3.6.4",
    "d3": "5.0.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.4",
    "multiclass-density-maps": "~0.1.5",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "webcola": "^3.4.0"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  }
}
